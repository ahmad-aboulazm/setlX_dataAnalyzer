addDefinedVariables := procedure(l, rw definedVars, isClass, className, rw allClassesIdenteties, isProcedure, classCheck) {	
//print("addDefinedVariables : " + canonical(l));
	match(l){
	
	case ^block(x):
		for(y in x){
			addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		}
		return;
	
	case ^variable(x):
		return;											   
	
	case ^assignment(^variable(x), ^procedure(y, z)):
		return;   //These 3 cases are only reached when checking classes, otherwise in any other
	
	case ^assignment(^variable(x), ^cachedProcedure(y, z)):
		return;	  //case the checkProcedure function is directly called from the checkCode function
	
	case ^assignment(^variable(x), ^lambdaDefinition(y, z)):
		return;   //so in this case we just have to ignore them.
	
	
	case ^assignment(^variable(x), ^call(y, z)):
		definedVars := definedVars + {x};
		return;
	
	case ^assignment(^variable(x), y):
		definedVars := definedVars + {x};
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		if(isClass && (x in allClassesIdenteties[className][2]) && isProcedure){
			print("Warning: A new variable $x$ is being defined instead of assigning the member variable of class 
				$className$, if you wish to assign the member variable add a prefix this. to it.");
		}
		return;
	
	case ^assignment(^memberAccess(x, y), _):
		if(classCheck && str(x) == "this"){
			definedVars := definedVars + {str(y)};
		}
		return; 
	
	case ^assignment(^assignListConstructor([a|b]), y):
		for(z in [a|b]){
			addDefinedVariables(^assignment(z,2), definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		}
		return; //Just putting any fake assignment for the 
				//variable to be counted as a defined variable.
	
	case ^assignment(x, y):
		return;
	
	//Mathematics
	case ^difference(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^sum(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^sumAssignment(x, y):
		if(isClass && (str(x) in allClassesIdenteties[className][2]) && isProcedure){
			print("Warning: A new variable $x$ is being defined instead of assigning the member variable of class $className$,
					if you wish to assign the member variable add a prefix this. to it.");
		}
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck); return;
	
	case ^differenceAssignment(x, y):
		if(isClass && (str(x) in allClassesIdenteties[className][2]) && isProcedure){
			print("Warning: A new variable $x$ is being defined instead of assigning the member variable of class $className$, 
					if you wish to assign the member variable add a prefix this. to it.");
		}
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck); return;
	
	case ^quotientAssignment(x, y):
		if(isClass && (str(x) in allClassesIdenteties[className][2]) && isProcedure){
			print("Warning: A new variable $x$ is being defined instead of assigning the member variable of class $className$,
					if you wish to assign the member variable add a prefix this. to it.");
		}
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck); return;	
	
	case ^integerDivision(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^quotient(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^product(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^modulo(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^power(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	//Booleans
	case ^equals(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^notEqual(x, y): 
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^greaterThan(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^lessThan(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^lessOrEqual(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^greaterOrEqual(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^not(x):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;

	case ^iterator(x, y, z):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(z, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^range(x, y, z):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(z, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^conjunction(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^disjunction(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^in(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	//return, backtrack, continue, break, exit
	case ^return(x):	 return;
	
	case ^backtrack():	 return;
	
	case ^break():		 return;
	
	case ^continue():	 return;
	
	case ^exit():		 return;
	
	//call
	case ^call(_, y):	 return; // No use for it  here as I did consider all used and defined variables in the "getUsedVariables"
	
	//Iterators
	case {x}:
		//print("{x} is called in addDefinedVariables");
		return;
	
	case ^setlIteration(_,_,_):
		return; // Skip as everything concerning the iteration is recorded previously
	
	case ^setlIterator(_,_,_):
		return;
	
	//Other stuff
	case ^collectionAccess(x, y):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^collectionAccessRangeDummy():
		return;
	
	case ^cardinality(x):
		return; // It is only used but not defined.
	
	case ^exists(x, y):
		return;
	
	case ^forall(x, y):
		return;
	
	case ^sumOfMembers([a|b]):
		for(y in [a|b]){
			addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);	
		}
		return;
	
	case ^sumOfMembersBinary(x, [a|b]):
		addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		for(y in [a|b]){
			addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);	
		}
		return;
	
	case ^explicitListWithRest([a|b], y):
		for(x in [a|b]){
			addDefinedVariables(x, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		}
		addDefinedVariables(y, definedVars, isClass, className, allClassesIdenteties, isProcedure, classCheck);
		return;
	
	case ^memberAccess(_,_):
		return; //Accessing class variables only uses objects and might define their class variables BUT never defines any external variable
	
	default:
		//print("Fakes in addDefinedVariables :\""+ canonical(l) + "\"");
		return;
	}

};

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
checkCode := procedure(l, rw definedVars, rw allUsedVars, isProcedure, procName, procParameters, rw procEmbeddedProcedures,
				rw allProceduresIdenteties, forLoopCase, rw forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, rw allClassesIdenteties) {
//print("checkCode : " + canonical(l));

match(l){
	case ^block(l2):  		
		for(y in l2){
			checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures,
				allProceduresIdenteties, forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		}
		return;

	case ^assignment(^variable(x), ^procedure(y, z)):
		if(!isProcedure){
			if(!isClass){
				checkProcedure(x,^procedure(y,z), allProceduresIdenteties, allClassesIdenteties,{}, isClass, className);
			}
			else{
			    checkProcedure(x,^procedure(y,z), allProceduresIdenteties, allClassesIdenteties, allClassesIdenteties[className][2] + {"this"},
							isClass, className);															   
			}
		}
		else{
			procEmbeddedProcedures += {x};
			definedVars := definedVars + {x};
			allUsedVars += {x};
			getProcedureParameters(y, definedVars, {});
			checkCode(z, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties,
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		}
		return;
	
	case ^assignment(^variable(x), ^cachedProcedure(y, z)) :	
		if(!isProcedure){
			if(!isClass){
				checkProcedure(x,^procedure(y,z), allProceduresIdenteties, allClassesIdenteties, {}, isClass, className);
			}
			else{
			    checkProcedure(x,^procedure(y,z), allProceduresIdenteties, allClassesIdenteties,
							allClassesIdenteties[className][2] + {"this"}, isClass, className);
			}
		}
		else{
			procEmbeddedProcedures += {x};
			definedVars := definedVars + {x};
			allUsedVars += {x};
			getProcedureParameters(y, definedVars, {});
			checkCode(z, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties,
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		}
		return;
	
	case ^assignment(^variable(x), ^lambdaDefinition(y, z))	:	
		if(!isProcedure){
			if(!isClass){
			    checkProcedure(x, ^procedure(y, ^block([z])), allProceduresIdenteties, allClassesIdenteties, {}, isClass, className);
			}
			else{
				checkProcedure(x, ^procedure(y, ^block([z])), allProceduresIdenteties, allClassesIdenteties,
							allClassesIdenteties[className][2] + {"this"}, isClass, className);
			}
		}
		else{
			procEmbeddedProcedures += {x};
			definedVars := definedVars + {x};
			allUsedVars += {x};
			getProcedureParameters(y, definedVars, {});
			checkCode(z, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties,
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		}
		return;
																
	//Adding members to existing objects
	case ^assignment(^memberAccess(^variable(a),^variable(b)), ^procedure(y, z)) :	
		newClassName := "extra-" + str(a);
		//print(newClassName);
		if(!(newClassName in domain(allClassesIdenteties))){
			oldClassProperties := {oc : oc in allClassesIdenteties | str(a) in oc[2][4]};
			if(#oldClassProperties != 0){
			    oldClassProperties := first(oldClassProperties);
				oldClassProperties := oldClassProperties[2];
			}
			else{
				a := {f : g in allClassesIdenteties, f in g[2][1]};
				b := {f : g in allClassesIdenteties, f in g[2][2]};
				c := {f : g in allClassesIdenteties, f in g[2][3]};
				d := {f : g in allClassesIdenteties, f in g[2][4]};
				oldClassProperties := [a, b, c, d];
			}
			oldClassProperties[4] := {str(a)};
			//print(oldClassProperties);
			checkProcedure(b, ^procedure(y, z), oldClassProperties[3], allClassesIdenteties,
						oldClassProperties[2] + {"this"}, isClass, className);
			allClassesIdenteties := allClassesIdenteties + {[newClassName, oldClassProperties]};
		}
		else{
			checkProcedure(b, ^procedure(y, z), allClassesIdenteties[newClassName][3], allClassesIdenteties,
						allClassesIdenteties[newClassName][2] + {"this"}, isClass, className);
		}
		allClassesIdenteties[newClassName][3] += {[str(b),[]]};
		getProcedureIdentity(str(b), ^procedure(y, z), allClassesIdenteties[newClassName][3]);
		return;
	
	case ^assignment(^memberAccess(^variable(a),^variable(b)), ^cachedProcedure(y, z)) :	
		newClassName := "extra-" + str(a);
		//print(newClassName);
		if(!(newClassName in domain(allClassesIdenteties))){
			oldClassProperties := {oc : oc in allClassesIdenteties | str(a) in oc[2][4]};
			if(#oldClassProperties != 0){
				oldClassProperties := first(oldClassProperties);
				oldClassProperties := oldClassProperties[2];
			}
			else{
				a := {f : g in allClassesIdenteties, f in g[2][1]};
				b := {f : g in allClassesIdenteties, f in g[2][2]};
				c := {f : g in allClassesIdenteties, f in g[2][3]};
				d := {f : g in allClassesIdenteties, f in g[2][4]};
				oldClassProperties := [a, b, c, d];
			}
			oldClassProperties[4] := {str(a)};
			//print(oldClassProperties);
			checkProcedure(b, ^procedure(y, z), oldClassProperties[3], allClassesIdenteties,
						oldClassProperties[2] + {"this"}, isClass, className);
			allClassesIdenteties += [newClassName, oldClassProperties];
		}
		else{
			checkProcedure(b, ^procedure(y, z), allClassesIdenteties[newClassName][3], allClassesIdenteties, 
						allClassesIdenteties[newClassName][2] + {"this"}, isClass, className);
		}
		allClassesIdenteties[newClassName][3] += {[str(b),[]]};
		getProcedureIdentity(str(b), ^procedure(y, z), allClassesIdenteties[newClassName][3]);
		return;	
																
	case ^assignment(^memberAccess(^variable(a),^variable(b)), ^lambdaDefinition(y, z))	:
		newClassName := "extra-" + str(a);
		//print(newClassName);
		if(!(newClassName in domain(allClassesIdenteties))){
			oldClassProperties := {oc : oc in allClassesIdenteties | str(a) in oc[2][4]};
			if(#oldClassProperties != 0){
				oldClassProperties := first(oldClassProperties);
				oldClassProperties := oldClassProperties[2];
			}
			else{
				a := {f : g in allClassesIdenteties, f in g[2][1]};
				b := {f : g in allClassesIdenteties, f in g[2][2]};
				c := {f : g in allClassesIdenteties, f in g[2][3]};
				d := {f : g in allClassesIdenteties, f in g[2][4]};
				oldClassProperties := [a, b, c, d];
			}
			oldClassProperties[4] := {str(a)};
			//print(oldClassProperties);
			checkProcedure(b, ^procedure(y, ^block([z])), oldClassProperties[3], allClassesIdenteties,
						oldClassProperties[2] + {"this"}, isClass, className);
			allClassesIdenteties += [newClassName, oldClassProperties];
		}
		else{
			checkProcedure(b, ^procedure(y, ^block([z])), allClassesIdenteties[newClassName][3], allClassesIdenteties,
						allClassesIdenteties[newClassName][2] + {"this"}, isClass, className);
		}
		allClassesIdenteties[newClassName][3] += {[str(b),[]]};
		getProcedureIdentity(str(b), ^procedure(y, ^block([z])), allClassesIdenteties[newClassName][3]);
		return;
	
	case ^ifThen(x): 
		for(y in x) 
		{
			checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties,
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		} 
		return;
	
	case ^ifThenElseIfBranch(x, y): 
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties); 
		checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties); 
		return;
	
	case ^ifThenBranch(cond, then): 
		checkCode(cond, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		checkCode(then, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		return;
	
	case ^ifThenElseBranch(x): 
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		return;
									   
	case ^while(x, y): 
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		return;
	
	case ^for(x, y, z): 
		forLoopCase := true;
		temp := forLoopTmpDefVars;
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		checkCode(z, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties); 
		forLoopTmpDefVars := temp; //return it to the way it was in case they were nested forloops.
		forLoopCase := false;
		return;
	
	case ^tryCatch(x, [a|b]): 
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		for(y in [a|b]){
		    checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
						forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		}
		return;
	
	case ^tryCatch(x, y): 
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		return;
	
	case ^tryCatchLngBranch(x, y): 
		addDefinedVariables(^assignment(x,1), exceptionTmp, isClass, className, allClassesIdenteties, isProcedure, false);
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties,
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		return;
	
	case ^tryCatchUsrBranch(x, y): 
		addDefinedVariables(^assignment(x,1), exceptionTmp, isClass, className, allClassesIdenteties, isProcedure, false);
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		checkCode(y, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties, 
					forLoopCase, forLoopTmpDefVars, exceptionTmp, matchTmp, isClass, className, allClassesIdenteties);
		return;
	
	case ^tryCatchBranch(x, y): 
		addDefinedVariables(^assignment(x,1), exceptionTmp, isClass, className, allClassesIdenteties, isProcedure, false);
		checkCode(x, definedVars, allUsedVars, isProcedure, procName, procParameters, procEmbeddedProcedures, allProceduresIdenteties,
