//Getting the file directory of the file to check
main := procedure(params){
for(directory in params){
	print("NOW CHECKING $directory$");
	directory := str(directory);
	l := readFile(directory); 
	l := +/ [ g + "\n" : g in l ]; // Adding a new line to the end of all lines to avoid getting problems from the commands just like this one "//"
	l := parseStatements(l);

	definedVars := {};
	allUsedVars := {};
	allProceduresIdenteties := {};
	allClassesIdenteties := {};
	
	
	identifyAllProcedures(l, allProceduresIdenteties);  // To avoid the calling cycles ! So we get all procedures' identities first before checking the code
	identifyAllClasses(l, allClassesIdenteties);
	analyzeAllLoadedFiles(l, definedVars, allUsedVars, allProceduresIdenteties, allClassesIdenteties);
	checkCode(l, definedVars, allUsedVars, false, "", {}, {}, allProceduresIdenteties, false, {}, {}, {}, false, "", allClassesIdenteties);
	unUsedVars := definedVars-allUsedVars;
	if(#unUsedVars != 0)
	{
		if(#unUsedVars == 1){
		   print("Variable " + unUsedVars + " is defined but never used in the whole code! unless it(they) was(were) assigned to a returned procedure");
		}
		else{
		   print("Variables " + unUsedVars + " are defined but never used in the whole code! unless it(they) was(were) assigned to a returned procedure");
		
		}
	}
	print("DONE CHECKING $directory$");
}
};

main(params);